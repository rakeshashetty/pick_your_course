= simple_form_for @membership, :html => { :class => 'form-horizontal' } do |f|
  = f.association :user
  = f.association :role
  = f.association :program
  .form-actions
    = f.button :submit, :class => 'btn-primary'
    = link_to t('.cancel', :default => t("helpers.links.cancel")), memberships_path, :class => 'btn'
